AAA,EAAA,GAAA;AAAA,QAAA,QAAA,EAAA;AAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA;AACA,OAFA;AAGA,KA5CA;AA8CA,IAAA,kBA9CA,8BA8CA,YA9CA,EA8CA;AACA,UAAA,WAAA,GAAA,KAAA,KAAA,CAAA,IAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA,KAAA,YAAA;AAAA,OAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,WAAA;AACA,WAAA,eAAA,GAAA,WAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KAnDA;AAqDA,IAAA,gBArDA,8BAqDA;AAAA;;AACA,MAAA,gBAAA,CAAA,KAAA,eAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,GAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,UAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,KAAA;;AACA,UAAA,MAAA,CAAA,WAAA;AACA,SAHA,EAGA,IAHA,CAAA;AAIA,OATA;AAUA,KAhEA;AAkEA,IAAA,oBAlEA,kCAkEA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KApEA;AAsEA,IAAA,sBAtEA,oCAsEA;AAAA;;AACA,MAAA,gBAAA,CAAA,KAAA,eAAA,CAAA,EAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,GAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA;;AACA,QAAA,MAAA,CAAA,WAAA;;AACA,QAAA,UAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,SAFA,EAEA,IAFA,CAAA;AAGA,OATA;AAUA;AAjFA,GApCA;AAwHA,EAAA,OAxHA,qBAwHA;AACA,SAAA,WAAA;AACA,SAAA,UAAA;AACA;AA3HA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <v-container class=\"pl-16 pr-16\">\r\n      <!-- alert-->\r\n      <v-alert\r\n        class=\"alert\"\r\n        outlined\r\n        type=\"success\"\r\n        text\r\n        v-show=\"showAlert\"\r\n        transition=\"scroll-y-transition\"\r\n      >\r\n        {{ alertMsg }}\r\n      </v-alert>\r\n\r\n      <!-- 删除 dialog-->\r\n      <template>\r\n        <v-row justify=\"center\">\r\n          <v-dialog v-model=\"deleteDialog\" persistent max-width=\"600px\">\r\n            <v-card>\r\n              <v-card-title>\r\n                <span class=\"headline\">删除课件</span>\r\n              </v-card-title>\r\n              <v-card-text>\r\n                是否确认删除课件 {{ currentEditFile.name }} ?\r\n              </v-card-text>\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">\r\n                  取消\r\n                </v-btn>\r\n                <v-btn\r\n                  color=\"red darken-1\"\r\n                  text\r\n                  @click=\"handleDeleteCourseWare\"\r\n                >\r\n                  删除\r\n                </v-btn>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n        </v-row>\r\n      </template>\r\n\r\n      <!-- 编辑 dialog-->\r\n      <template>\r\n        <v-row justify=\"center\">\r\n          <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\r\n            <v-card>\r\n              <v-card-title>\r\n                <span class=\"headline\">编辑课件</span>\r\n              </v-card-title>\r\n              <v-card-text>\r\n                <v-container>\r\n                  <v-row>\r\n                    <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                      <v-text-field\r\n                        label=\"文件类型\"\r\n                        v-model=\"currentEditFile.fileType\"\r\n                        disabled\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                    <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                      <v-text-field\r\n                        label=\"文件大小\"\r\n                        v-model=\"currentEditFile.fileSize\"\r\n                        disabled\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                    <v-col cols=\"12\">\r\n                      <v-text-field\r\n                        label=\"文件名\"\r\n                        v-model=\"currentEditFile.fileName\"\r\n                        disabled\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                    <v-col cols=\"12\">\r\n                      <v-text-field\r\n                        label=\"课件标题\"\r\n                        v-model=\"currentEditFile.title\"\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                    <v-col cols=\"12\" sm=\"6\">\r\n                      <v-select\r\n                        :items=\"[true, false]\"\r\n                        label=\"是否免费\"\r\n                        v-model=\"currentEditFile.freeFlag\"\r\n                      ></v-select>\r\n                    </v-col>\r\n                    <v-col cols=\"12\" sm=\"6\">\r\n                      <v-select\r\n                        :items=\"[true, false]\"\r\n                        label=\"是否可下载\"\r\n                        v-model=\"currentEditFile.downloadFlag\"\r\n                      ></v-select>\r\n                    </v-col>\r\n                    <v-col cols=\"12\" sm=\"6\">\r\n                      <v-select\r\n                        :items=\"[true, false]\"\r\n                        label=\"是否有权查看\"\r\n                        v-model=\"currentEditFile.availableFlag\"\r\n                      ></v-select>\r\n                    </v-col>\r\n                  </v-row>\r\n                  <v-row class=\"pa-2\">\r\n                    <v-btn color=\"error\" @click=\"showDeleteFileDialog\">\r\n                      删除课件\r\n                    </v-btn>\r\n                  </v-row>\r\n                </v-container>\r\n              </v-card-text>\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">\r\n                  Close\r\n                </v-btn>\r\n                <v-btn color=\"blue darken-1\" text @click=\"handleEditSubmit\">\r\n                  Save\r\n                </v-btn>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-dialog>\r\n        </v-row>\r\n      </template>\r\n\r\n      <v-row>\r\n        <!-- 课件列表 -->\r\n        <v-col>\r\n          <v-card max-width=\"600\" class=\"mx-auto mt-8\">\r\n            <v-toolbar color=\"primary\" dark>\r\n              <v-app-bar-nav-icon></v-app-bar-nav-icon>\r\n              <v-toolbar-title>课程课件</v-toolbar-title>\r\n              <v-spacer></v-spacer>\r\n\r\n              <v-file-input\r\n                hide-input\r\n                prepend-icon=\"mdi-plus\"\r\n                v-model=\"instructionFile\"\r\n                @change=\"handleUploadFile\"\r\n              ></v-file-input>\r\n            </v-toolbar>\r\n\r\n            <v-list subheader two-line>\r\n              <v-list-item v-for=\"file in files\" :key=\"file.title\">\r\n                <v-list-item-avatar>\r\n                  <v-icon class=\"grey lighten-1\" dark>\r\n                    mdi-file\r\n                  </v-icon>\r\n                </v-list-item-avatar>\r\n\r\n                <v-list-item-content>\r\n                  <v-list-item-title v-text=\"file.title\"></v-list-item-title>\r\n\r\n                  <v-list-item-subtitle\r\n                    v-text=\"file.fileSize\"\r\n                  ></v-list-item-subtitle>\r\n                </v-list-item-content>\r\n\r\n                <v-list-item-action>\r\n                  <v-btn icon>\r\n                    <v-icon\r\n                      color=\"grey lighten-1\"\r\n                      @click=\"showEditFileDialog(file.id)\"\r\n                      >mdi-more</v-icon\r\n                    >\r\n                  </v-btn>\r\n                </v-list-item-action>\r\n              </v-list-item>\r\n            </v-list>\r\n          </v-card>\r\n        </v-col>\r\n        <!-- 课程信息 -->\r\n        <v-col>\r\n          <form class=\"pa-12 grey lighten-5 mt-8\">\r\n            <v-text-field\r\n              v-model=\"courseInfo.name\"\r\n              label=\"课程名称\"\r\n              readonly\r\n            ></v-text-field>\r\n            <v-text-field\r\n              v-model=\"courseInfo.id\"\r\n              label=\"课程ID\"\r\n              readonly\r\n            ></v-text-field>\r\n            <v-select\r\n              :items=\"types\"\r\n              v-model=\"courseInfo.type\"\r\n              label=\"课程分类\"\r\n              readonly\r\n            ></v-select>\r\n            <v-textarea\r\n              v-model=\"courseInfo.intro\"\r\n              label=\"课程简介\"\r\n              readonly\r\n            ></v-textarea>\r\n            <v-text-field\r\n              v-model=\"courseInfo.school\"\r\n              label=\"所属学校\"\r\n              readonly\r\n            ></v-text-field>\r\n            <v-text-field\r\n              v-model=\"courseInfo.createTime\"\r\n              label=\"创建时间\"\r\n              readonly\r\n            ></v-text-field>\r\n            <v-text-field\r\n              v-model=\"courseInfo.teacherName\"\r\n              label=\"创建教师\"\r\n              readonly\r\n            ></v-text-field>\r\n            <v-switch\r\n              v-model=\"isCharge\"\r\n              :label=\"`是否为付费课程: ${isCharge.toString()}`\"\r\n              readonly\r\n            ></v-switch>\r\n            <v-text-field\r\n              v-show=\"isCharge\"\r\n              v-model=\"courseInfo.cost\"\r\n              label=\"课程价格(元)\"\r\n              readonly\r\n            ></v-text-field>\r\n          </form>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { uploadFile } from \"@/api/file\";\r\nimport {\r\n  getCourseWareByCourse,\r\n  createCourseWare,\r\n  updateCourseWare,\r\n  deleteCourseWare\r\n} from \"@/api/courseWare\";\r\nimport { getCourseById } from \"@/api/course\";\r\n\r\nexport default {\r\n  name: \"CourseEdit\",\r\n\r\n  data() {\r\n    return {\r\n      courseInfo: {},\r\n      instructionFile: null,\r\n      types: [\"初级\", \"中级\", \"高级\"],\r\n      msg: \"\",\r\n      files: [],\r\n      showAlert: false,\r\n      alertMsg: \"\",\r\n      dialog: false,\r\n      deleteDialog: false,\r\n      currentEditFile: {\r\n        id: 0,\r\n        courseId: 0,\r\n        number: 0,\r\n        title: \"\",\r\n        fileName: \"\",\r\n        fileType: \"\",\r\n        fileSize: \"\",\r\n        freeFlag: false,\r\n        downloadFlag: true,\r\n        availableFlag: false,\r\n        uploadTime: \"\"\r\n      }\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    isCharge: function() {\r\n      return this.courseInfo.cost !== 0;\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    async handleUploadFile() {\r\n      const formData = new window.FormData();\r\n      formData.append(\"file\", this.instructionFile);\r\n      const res = await uploadFile(formData);\r\n      if (res.code === 1) {\r\n        const { courseId } = this.$route.params;\r\n        const fileInfo = {\r\n          courseId: courseId,\r\n          number: this.files.length,\r\n          title: res.data.name,\r\n          fileName: res.data.name,\r\n          fileType: res.data.type,\r\n          fileSize: res.data.size,\r\n          freeFlag: true,\r\n          downloadFlag: true,\r\n          availableFlag: true\r\n        };\r\n        createCourseWare(fileInfo).then(res => {\r\n          console.log(res);\r\n          this.alertMsg = res.msg;\r\n          this.showAlert = true;\r\n          setTimeout(() => {\r\n            this.showAlert = false;\r\n            this.refreshFile();\r\n          }, 1000);\r\n        });\r\n      }\r\n    },\r\n\r\n    refreshFile() {\r\n      const { courseId } = this.$route.params;\r\n      const uid = window.localStorage.getItem(\"userId\");\r\n      getCourseWareByCourse({ courseId, uid }).then(res => {\r\n        this.files = res || [];\r\n      });\r\n    },\r\n\r\n    loadCourse() {\r\n      const { courseId } = this.$route.params;\r\n      const uid = window.localStorage.getItem(\"userId\");\r\n      getCourseById({ uid, courseId }).then(res => {\r\n        this.courseInfo = res;\r\n      });\r\n    },\r\n\r\n    showEditFileDialog(courseWareId) {\r\n      const currentFile = this.files.find(f => f.id === courseWareId);\r\n      console.log(currentFile);\r\n      this.currentEditFile = currentFile;\r\n      this.dialog = true;\r\n    },\r\n\r\n    handleEditSubmit() {\r\n      updateCourseWare(this.currentEditFile).then(res => {\r\n        this.dialog = false;\r\n        console.log(res);\r\n        this.alertMsg = res.msg;\r\n        this.showAlert = true;\r\n        setTimeout(() => {\r\n          this.showAlert = false;\r\n          this.refreshFile();\r\n        }, 1000);\r\n      });\r\n    },\r\n\r\n    showDeleteFileDialog() {\r\n      this.deleteDialog = true;\r\n    },\r\n\r\n    handleDeleteCourseWare() {\r\n      deleteCourseWare(this.currentEditFile.id).then(res => {\r\n        this.alertMsg = res.msg;\r\n        this.deleteDialog = false;\r\n        this.dialog = false;\r\n        this.showAlert = true;\r\n        this.refreshFile();\r\n        setTimeout(() => {\r\n          this.showAlert = false;\r\n        }, 1000);\r\n      });\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.refreshFile();\r\n    this.loadCourse();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.alert {\r\n  position: fixed;\r\n  left: 50%;\r\n  top: 100px;\r\n  z-index: 999;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/teacher"}]}