AA;AARA,OAVA;AAoBA,MAAA,IAAA,EAAA;AACA,QAAA,SAAA,EAAA,CADA;AAEA,QAAA,OAAA,EAAA,CAFA;AAGA,QAAA,QAAA,EAAA,CAHA;AAIA,QAAA,QAAA,EAAA,EAJA;AAKA,QAAA,SAAA,EAAA,EALA;AAMA,QAAA,QAAA,EAAA;AANA,OApBA;AA6BA,MAAA,iBAAA,EAAA,KA7BA;AA8BA,MAAA,cAAA,EAAA,KA9BA;AA+BA,MAAA,GAAA,EAAA;AA/BA,KAAA;AAiCA,GArCA;AAuCA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AAAA;;AACA,UAAA,OAAA,mCACA,KAAA,UADA;AAEA,QAAA,IAAA,EAAA,KAAA,SAAA,CAAA,KAAA,QAAA,CAAA,OAAA,CAAA,KAAA,UAAA,CAAA,IAAA,CAAA,CAFA;AAGA,QAAA,KAAA,EAAA,KAAA,UAAA,CAAA,KAAA,SAAA,CAAA,OAAA,CAAA,KAAA,UAAA,CAAA,KAAA,CAAA,CAHA;AAIA,QAAA,QAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,IAAA;AAJA,QAAA;;AAMA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,MAAA,YAAA,CAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,iBAAA,GAAA,IAAA;AACA,UAAA,UAAA,CAAA,YAAA;AACA,YAAA,KAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,YAAA,KAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA,WAHA,EAGA,IAHA,CAAA;AAIA,SANA,MAMA;AACA,UAAA,KAAA,CAAA,cAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,GAAA,GAAA,GAAA,CAAA,GAAA;AACA,UAAA,UAAA,CAAA,YAAA;AACA,YAAA,KAAA,CAAA,cAAA,GAAA,KAAA;AACA,WAFA,EAEA,IAFA,CAAA;AAGA;AACA,OAfA;AAgBA;AAzBA;AAvCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- alert -->\r\n    <v-alert\r\n      class=\"alert\"\r\n      outlined\r\n      type=\"success\"\r\n      text\r\n      v-show=\"showSuccessDialog\"\r\n      transition=\"scroll-y-transition\"\r\n    >\r\n      优惠创建成功！\r\n    </v-alert>\r\n    <!-- alert -->\r\n    <v-alert\r\n      class=\"alert\"\r\n      outlined\r\n      type=\"warning\"\r\n      text\r\n      v-show=\"showFailDialog\"\r\n      transition=\"scroll-y-transition\"\r\n    >\r\n      {{ msg }}\r\n    </v-alert>\r\n    <v-container class=\"pl-16 pr-16\">\r\n      <form class=\"pa-12 grey lighten-5 mt-8\">\r\n        <v-text-field v-model=\"couponInfo.name\" label=\"优惠名称\"></v-text-field>\r\n        <v-select\r\n          :items=\"typeList\"\r\n          v-model=\"couponInfo.type\"\r\n          label=\"优惠分类\"\r\n        ></v-select>\r\n        <v-text-field\r\n          v-show=\"couponInfo.type === 'DISCOUNT'\"\r\n          v-model=\"meta.discount\"\r\n          label=\"折扣数额（0 ～ 1）\"\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-show=\"couponInfo.type === 'CUT_DOWN'\"\r\n          v-model=\"meta.threshold\"\r\n          label=\"满减门槛\"\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-show=\"couponInfo.type === 'CUT_DOWN'\"\r\n          v-model=\"meta.cutDown\"\r\n          label=\"满减金额\"\r\n        ></v-text-field>\r\n        <v-select\r\n          :items=\"scopeList\"\r\n          v-model=\"couponInfo.scope\"\r\n          label=\"使用范围\"\r\n        ></v-select>\r\n        <v-text-field\r\n          v-show=\"couponInfo.scope === 'SINGLE'\"\r\n          v-model=\"meta.teacherId\"\r\n          label=\"课程ID\"\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-show=\"couponInfo.scope === 'TEACHER'\"\r\n          v-model=\"meta.teacherId\"\r\n          label=\"教师ID\"\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-show=\"couponInfo.scope === 'SCHOOL'\"\r\n          v-model=\"meta.teacherId\"\r\n          label=\"教师ID\"\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"couponInfo.description\"\r\n          label=\"优惠说明\"\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"couponInfo.startTime\"\r\n          label=\"开始时间\"\r\n          messages=\"YYYY-MM-DD HH:MM:SS\"\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"couponInfo.endTime\"\r\n          label=\"结束时间\"\r\n          messages=\"YYYY-MM-DD HH:MM:SS\"\r\n        ></v-text-field>\r\n        <v-switch\r\n          v-model=\"couponInfo.sharable\"\r\n          :label=\"`是否可与其他优惠同时使用: ${couponInfo.sharable.toString()}`\"\r\n        ></v-switch>\r\n        <v-btn class=\"ml-0 mt-8 info\" @click=\"submit\">\r\n          确认\r\n        </v-btn>\r\n      </form>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { createCoupon } from \"@/api/coupon\";\r\n\r\nexport default {\r\n  name: \"CouponCreate\",\r\n\r\n  data() {\r\n    return {\r\n      typeList_: [\"CUT_DOWN\", \"DISCOUNT\"],\r\n      typeList: [\"满减\", \"折扣\"],\r\n      scopeList_: [\"SINGLE\", \"TEACHER\", \"SCHOOL\", \"COMMON\"],\r\n      scopeList: [\r\n        \"单一课程\",\r\n        \"指定教师的课程\",\r\n        \"指定学习的课程\",\r\n        \"全站课程通用\"\r\n      ],\r\n      couponInfo: {\r\n        type: \"满减\", // \"DISCOUNT\"\r\n        scope: \"单一课程\", // \"TEACHER\" \"COMMON\"\r\n        name: \"\",\r\n        description: \"\",\r\n        metadata: \"\",\r\n        startTime: \"\",\r\n        endTime: \"\",\r\n        sharable: false\r\n      },\r\n      meta: {\r\n        threshold: 0,\r\n        cutDown: 0,\r\n        discount: 0,\r\n        courseId: \"\",\r\n        teacherId: \"\",\r\n        schoolId: \"\"\r\n      },\r\n\r\n      showSuccessDialog: false,\r\n      showFailDialog: false,\r\n      msg: \"\"\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    submit() {\r\n      const payload = {\r\n        ...this.couponInfo,\r\n        type: this.typeList_[this.typeList.indexOf(this.couponInfo.type)],\r\n        scope: this.scopeList_[this.scopeList.indexOf(this.couponInfo.scope)],\r\n        metadata: JSON.stringify(this.meta)\r\n      };\r\n      console.log(payload);\r\n      createCoupon(payload).then(res => {\r\n        console.log(res);\r\n        if (res.code === 1) {\r\n          this.showSuccessDialog = true;\r\n          setTimeout(() => {\r\n            this.showSuccessDialog = false;\r\n            this.$router.go(-1);\r\n          }, 1000);\r\n        } else {\r\n          this.showFailDialog = true;\r\n          this.msg = res.msg;\r\n          setTimeout(() => {\r\n            this.showFailDialog = false;\r\n          }, 1000);\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.alert {\r\n  position: fixed;\r\n  left: 50%;\r\n  top: 100px;\r\n  z-index: 999;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/admin"}]}