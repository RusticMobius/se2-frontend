indow.localStorage.removeItem(\"userId\");\r\n      window.localStorage.removeItem(\"userPhone\");\r\n      window.localStorage.removeItem(\"username\");\r\n      window.localStorage.removeItem(\"userRole\");\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.cursor {\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/layouts"}]}